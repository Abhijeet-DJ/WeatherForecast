<script>
  function drawGauge(uvValue) {
    const canvas = document.getElementById("GaugeUvIndex");
    const ctx = canvas.getContext("2d");

    const centerX = canvas.width / 2;
    const centerY = canvas.height / 2;
    const radius = 100;
    const startAngle = Math.PI; // Left side
    const endAngle = 2 * Math.PI; // Right side

    // Clear canvas
    ctx.clearRect(0, 0, canvas.width, canvas.height);

    // Draw gauge sections (Green, Yellow, Red)
    const sections = [
      { color: "green", start: Math.PI, end: 4 * Math.PI / 3 },
      { color: "yellow", start: 4 * Math.PI / 3, end: 5 * Math.PI / 3 },
      { color: "red", start: 5 * Math.PI / 3, end: 2 * Math.PI }
    ];

    sections.forEach(section => {
      ctx.beginPath();
      ctx.arc(centerX, centerY, radius, section.start, section.end);
      ctx.lineWidth = 20;
      ctx.strokeStyle = section.color;
      ctx.stroke();
    });

    // Draw needle
    const uvNormalized = (uvValue / 10) * (endAngle - startAngle) + startAngle;
    const needleLength = radius - 20;
    const needleX = centerX + needleLength * Math.cos(uvNormalized);
    const needleY = centerY + needleLength * Math.sin(uvNormalized);

    ctx.beginPath();
    ctx.moveTo(centerX, centerY);
    ctx.lineTo(needleX, needleY);
    ctx.lineWidth = 4;
    ctx.strokeStyle = "black";
    ctx.stroke();

    // Draw UV Index label
    ctx.font = "20px Arial";
    ctx.fillStyle = "white";
    ctx.fillText(`UV Index: ${uvValue}`, centerX - 40, centerY + 25);
  }

  // Example: Set UV Index (change this value to update the gauge)
  drawGauge(6);
</script>

