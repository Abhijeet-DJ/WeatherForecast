<script>
  // Include this script in your HTML file

  // Import THREE.js if using modules
  // import * as THREE from 'https://cdn.jsdelivr.net/npm/three@0.137.0/build/three.module.js';

  // Function to initialize the 3D globe
  function initGlobe() {
    const container = document.getElementById("globe-box");
    const width = container.clientWidth;
    const height = container.clientHeight;

    // Create scene
    const scene = new THREE.Scene();

    // Create camera
    const camera = new THREE.PerspectiveCamera(80, width / height, 0.1, 1000);
    camera.position.z = 2;

    // Create renderer
    const renderer = new THREE.WebGLRenderer({ alpha: true });
    renderer.setSize(width, height);
    container.appendChild(renderer.domElement);

    // Create sphere (globe)
    const geometry = new THREE.SphereGeometry(1.25, 84, 84);
    const textureLoader = new THREE.TextureLoader();
    const texture = textureLoader.load('globe.jpg');
    const material = new THREE.MeshStandardMaterial({
      map: texture,
      emissive: 0xB2CDEA, // Blue glow effect
      emissiveIntensity: 0.1, // Adjust glow intensity
      roughness: 0,
      metalness: 0,
    });
    const globe = new THREE.Mesh(geometry, material);
    scene.add(globe);

    // Add lighting
    const light = new THREE.PointLight(0xffffff, 1, 100);
    light.position.set(0, 0, 15);
    scene.add(light);

    const ambientLight = new THREE.AmbientLight(0x404040, 2); // Soft ambient light
    scene.add(ambientLight);

    // Animation loop
    function animate() {
      requestAnimationFrame(animate);
      globe.rotation.y += 0.0025; // Rotate globe
      renderer.render(scene, camera);
    }
    animate();

    // Handle window resize
    window.addEventListener('resize', () => {
      const newWidth = container.clientWidth;
      const newHeight = container.clientHeight;
      renderer.setSize(newWidth, newHeight);
      camera.aspect = newWidth / newHeight;
      camera.updateProjectionMatrix();
    });
  }

  // Load Three.js and initialize the globe
  (function loadThreeJS() {
    const script = document.createElement('script');
    script.src = 'https://cdnjs.cloudflare.com/ajax/libs/three.js/r128/three.min.js';
    script.onload = initGlobe;
    document.head.appendChild(script);
  })();

</script>

